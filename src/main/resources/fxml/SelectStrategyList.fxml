<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SelectStrategyListController">
    <center>
        <ListView fx:id="strategyListView" prefHeight="600.0" prefWidth="500.0" BorderPane.alignment="CENTER" />
    </center>
    <bottom>
        <Button fx:id="okButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="OK" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </BorderPane.margin>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </Button>
    </bottom>
</BorderPane>
